{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      "single"
    ],
		"forin": false,
		"no-empty": false,
    "indent": [
      "tabs"
    ],
    "trailing-comma": false,
    "ordered-imports": false,
    "variable-name": [
      "ban-keywords"
    ],
    "object-literal-sort-keys": false,
    "no-debugger": false,
    "max-line-length": false,
    "only-arrow-functions": false,
    "member-access": [
      "check-accessor"
    ],
    "one-variable-per-declaration": false,
    "no-unused-expression": false,
    "no-bitwise": false
  }
}
